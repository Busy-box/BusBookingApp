@model IEnumerable<Tripodea.ServiceLayer.DTOs.Bus.ResultDto>
<div class="container">
    <label class="label label-info">
        Showing @Model.Count() journies&nbsp;
    </label>
    @foreach (var result in Model)
    {
        <div>
            <h4><a href="#">@result.JourneyFrom to @result.JourneyTo</a></h4>
            <h6>
                Available seats
                <span class="label label-success">@result.AvailableSeats</span> &nbsp;|&nbsp;
                @result.Bus &nbsp;|&nbsp;
                Departure: @result.Departure
                <br />
            </h6>
            <p>
                @result.Description
            </p>
        </div>
    }
    <br />
</div>
